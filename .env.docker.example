# Docker环境变量配置
# 复制此文件为 .env.docker 并填入实际值

# 阿里云OSS配置
ALIOSS_ACCESS_KEY_ID=your_access_key_id
ALIOSS_ACCESS_KEY_SECRET=your_access_key_secret  
ALIOSS_BUCKET_NAME=your_bucket_name
ALIOSS_REGION=your_region
ALIOSS_ENDPOINT=https://your_bucket.oss-region.aliyuncs.com

# 其他配置（可选，有默认值）
PORT=3000
CACHE_DURATION_DAYS=7
SCREENSHOT_TIMEOUT=30000
MAX_SCREENSHOT_WIDTH=1920
MAX_SCREENSHOT_HEIGHT=1080
